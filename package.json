{
  "name": "nasa-rss-app",
  "version": "1.0.0",
  "description": "NASA RSS App",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhamilton91/react-example"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "prebuild": "npm-run-all --parallel clean-dist lint",
    "build": "cross-env NODE_ENV=production node tools/build.js",
    "lint": "eslint --max-warnings 0 ./app --ext .js*",
    "lint:fix": "eslint --max-warnings 0 --fix ./app --ext .js*",
    "clean-dist": "npm run remove-dist & mkdir dist",
    "remove-dist": "rimraf ./dist",
    "start": "node localIndex.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart:dev": "npm-run-all --parallel start-message remove-dist",
    "start:dev": "cross-env NODE_ENV=development UV_THREADPOOL_SIZE=100 babel-node ./server/index.js",
    "debug": "cross-env NODE_ENV=development UV_THREADPOOL_SIZE=100 node --inspect --debug ./server/index.js",
    "test": "cross-env NODE_ENV=production jest --coverage",
    "test:debug": "cross-env NODE_ENV=production node --inspect --debug-brk ./node_modules/jest/bin/jest.js --runInBand --env jest-environment-node-debug"
  },
  "devDependencies": {
    "autoprefixer": "~6.4.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "~6.0.4",
    "babel-loader": "~6.0.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.3",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^1.1.3",
    "classnames": "~2.2.1",
    "compression": "^1.6.2",
    "cross-env": "~2.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "~3.2.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.24.1",
    "jest-environment-node-debug": "^2.0.0",
    "npm-run-all": "^3.1.1",
    "open": "0.0.5",
    "open-cli": "^1.0.5",
    "postcss-loader": "~0.10.1",
    "pre-commit": "^1.2.2",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "react-transform-catch-errors": "~1.0.0",
    "react-transform-hmr": "~1.0.1",
    "redbox-react": "~1.3.0",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "~6.1.4",
    "babel-preset-stage-2": "^6.18.0",
    "body-parser": "^1.15.2",
    "express": "~4.15.2",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "prop-types": "^15.5.8",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.5",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "semantic-ui-css": "^2.2.11",
    "semantic-ui-react": "^0.71.1",
    "xml2js": "^0.4.17"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "engines": {
    "node": "8.2.1"
  }
}
